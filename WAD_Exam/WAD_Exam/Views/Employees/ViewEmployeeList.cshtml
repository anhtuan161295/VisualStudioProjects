@model IEnumerable<WAD_Exam.Models.Employees>
@{
    ViewBag.Title = "View Employee List";
}

<h2>View Employee List</h2>
<p>
    @Html.ActionLink("Create Employee", "AddNewEmployee")
</p>
<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.EmpDoB)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.EmpName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmpEmail)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @*<td>
                @Html.DisplayFor(modelItem => item.EmpDoB)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.EmpName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpEmail)
            </td>
            <td>
                @Html.ActionLink("View Details", "EditOrDeleteEmployee", new { id = item.EmpID })
            </td>
        </tr>
    }
</table>